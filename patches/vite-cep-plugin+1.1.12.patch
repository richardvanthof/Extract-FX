+  </Resources>
+  <Lifecycle>
+    <AutoVisible>${autoVisible}</AutoVisible>${
+  (startOnEvents &&
+    `<StartOn>${startOnEvents
+      .map((event) => `\n<Event>${event}</Event>`)
+      .join("")}</StartOn>`) ||
+  ""
+} 
+  </Lifecycle>
+  <UI>
+    <Type>${type}</Type>
+    ${panelDisplayName ? `<Menu>${panelDisplayName}</Menu>` : ""}
